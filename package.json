    "@tailwindcss/forms": "^0.5.10",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.3"
  },
  "dependencies": {
    "ctf-student-platform": "file:"
  }
}
